{
  "version": 2,
  "builds": [
    {
      "src": "iphone_import_system/wsgi.py",
      "use": "@vercel/python",
      "config": { "maxLambdaSize": "15mb", "runtime": "python3.9" }
    },
    {
      "src": "build_files.sh",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "staticfiles_build"
      }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "iphone_import_system/wsgi.py"
    }
  ],
  "env": {
    "DEBUG": "False",
    "USE_SQLITE": "False"
  }
}
